{
  "name": "nodeopixxl",
  "version": "0.0.1",
  "description": "Addressing neopixels on a raspberry pi via nodejs",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "npm run build:clean && npm run build:webpack",
    "build:clean": "rimraf dist && rimraf build",
    "build:webpack": "node scripts/build.js",
    "fcserver:win": ".\\fcserver\\fcserver.exe .\\fcserver\\config.json",
    "fcserver:rpi": "./fcserver/fcserver-rpi ./fcserver/config.json",
    "wsserver": "node ./src/writer/index.js",
    "wsserver:dev": "nodemon --watch src ./src/writer/index.js",
    "wsserver:devlocal": "nodemon --watch src ./src/writer/index.js localclient"
  },
  "author": "Herr Zatacke",
  "private": true,
  "projectConfig": {
    "NUM_LEDS": 48
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "ndarray": "^1.0.18",
    "puppeteer": "^1.8.0",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "node-watch": "^0.5.8",
    "nodemon": "^1.18.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "reset-css": "^4.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.11.1",
    "webpack-dev-server": "^3.1.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  }
}
