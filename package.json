{
  "name": "nodeopixxl",
  "version": "0.0.2",
  "description": "Addressing neopixels on a raspberry pi via nodejs",
  "scripts": {
    "start": "webpack-dev-server --config ./scripts/webpack.dev.js",
    "host": "npm run build && node ./scripts/host.js",
    "build": "npm run build:clean && npm run build:webpack",
    "build:clean": "rimraf dist && rimraf build",
    "build:webpack": "webpack --config ./scripts/webpack.prod.js",
    "postinstall": "node scripts/postinstall/postinstall.js",
    "fcserver:win": ".\\fcserver\\fcserver.exe",
    "fcserver:rpi": "./fcserver/fcserver-rpi",
    "wsserver": "node ./src/writer/index.js",
    "wsserver:dev": "nodemon --watch src ./src/writer/index.js"
  },
  "author": "Herr Zatacke",
  "private": true,
  "projectConfig": {
    "NUM_LEDS": 160
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "chalk": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "express": "^4.17.1",
    "flatten": "^1.0.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^1.0.4",
    "ndarray": "^1.0.18",
    "node-sass": "^4.14.1",
    "node-watch": "^0.6.4",
    "nodemon": "^2.0.4",
    "param-case": "^3.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^5.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.4.5",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "reset-css": "^5.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "unzip-stream": "^0.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "ws": "^7.3.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.0",
    "utf-8-validate": "^5.0.1"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11"
  ]
}
